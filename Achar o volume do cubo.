#include <iostream>

using namespace std;

int main()
{
	/* 1.Utilize apenas uma instrução cin para ler as três informações abaixo para variáveis
       diferentes. Multiplique o valor das variáveis altura, largura e comprimento para
       achar o volume do cubo. */
	
	
	float resultado;
	float alt, larg, comp;

	cin >> alt >> larg >> comp;

	resultado = alt * larg * comp;

	cout << "O Volume eh "
		<< resultado
		<< "cm cubicos" << endl;

	cout << "*************";
